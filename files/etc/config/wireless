
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/soc/3f300000.mmc/mmc_host/mmc1/mmc1:0001/mmc1:0001:1'
	option htmode 'HT20'
	option disabled '0'
	option country 'US'
	option channel '1'

config wifi-iface
	option network 'wwan'
	option ssid 'ZenoTec'
	option encryption 'psk2'
	option device 'radio0'
	option mode 'sta'
	option key 'DEADBEEF'

