
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/soc/3f300000.mmc/mmc_host/mmc1/mmc1:0001/mmc1:0001:1'
	option htmode 'HT20'
	option disabled '0'
	option channel '9'
	option country 'US'

config wifi-iface
	option ssid 'ZENOTEC'
	option encryption 'psk2'
	option device 'radio0'
	option mode 'sta'
	option bssid '04:A1:51:BC:88:64'
	option key 'DEADBEEF'
	option network 'wlan lan'

